generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model caja_cabeza {
  DocumentoTipo       String             @db.VarChar(4)
  DocumentoSucursal   String             @db.VarChar(4)
  DocumentoNumero     String             @db.VarChar(10)
  FechaHora           DateTime?          @db.DateTime(0)
  CodigoUsuario       String?            @db.VarChar(10)
  ImporteInicial      Float?             @default(0.00)
  ImporteCierre       Float?             @default(0.00)
  ImporteCierreFisico Float?             @default(0.00)
  FechaHoraCierre     DateTime?          @db.DateTime(0)
  Observaciones       String?            @db.VarChar(250)
  caja_movimientos    caja_movimientos[] @ignore

  @@id([DocumentoTipo, DocumentoSucursal, DocumentoNumero])
  @@index([CodigoUsuario], map: "VendedorCodigo")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model caja_movimientos {
  DocumentoTipo       String      @db.VarChar(4)
  DocumentoSucursal   String      @db.VarChar(4)
  DocumentoNumero     String      @db.VarChar(10)
  CodigoUsuario       String      @db.VarChar(10)
  FechaHora           DateTime?   @db.DateTime(0)
  ConceptoCodigo      String?     @db.VarChar(4)
  ConceptoDescripcion String?     @db.VarChar(250)
  Importe             Float?
  Observacion         String?     @db.VarChar(250)
  caja_cabeza         caja_cabeza @relation(fields: [DocumentoTipo, DocumentoSucursal, DocumentoNumero], references: [DocumentoTipo, DocumentoSucursal, DocumentoNumero], onUpdate: Restrict, map: "caja_movimientos_fk")

  @@index([DocumentoTipo, DocumentoSucursal, DocumentoNumero], map: "DocumentoTipo")
  @@ignore
}

model cargapordefecto {
  CodigoUnidad      String            @default("") @db.VarChar(4)
  CodigoDia         String            @default("") @db.Char(1)
  CodigoArticulo    String            @db.VarChar(13)
  ArticuloCantidad  Float             @default(0)
  t_unidadesmoviles t_unidadesmoviles @relation(fields: [CodigoUnidad], references: [Codigo], onUpdate: Restrict, map: "cargapordefecto_fk1")
  t_articulos       t_articulos       @relation(fields: [CodigoArticulo], references: [Codigo], onUpdate: Restrict, map: "cargapordefecto_ibfk_1")
  t_dias            t_dias            @relation(fields: [CodigoDia], references: [Codigo], onUpdate: Restrict, map: "cargapordefecto_ibfk_2")

  @@id([CodigoUnidad, CodigoDia, CodigoArticulo])
  @@index([CodigoArticulo], map: "CodigoArticulo")
  @@index([CodigoDia], map: "CodigoDia")
  @@index([CodigoUnidad], map: "CodigoUnidad")
}

model cheques {
  Codigo                   Int              @id
  Numero                   String?          @db.VarChar(50)
  FechaEntrada             DateTime?        @db.Date
  FechaAcredita            DateTime?        @db.Date
  BancoCodigo              String?          @db.VarChar(20)
  Importe                  Float?           @default(0.00)
  ClienteCodigo            String?          @db.VarChar(10)
  DocumentoEntradaTipo     String?          @db.VarChar(4)
  DocumentoEntradaSucursal String?          @db.VarChar(4)
  DocumentoEntradaNumero   String?          @db.VarChar(8)
  ProveedorCodigo          String?          @db.VarChar(10)
  DocumentoSalidaTipo      String?          @db.VarChar(4)
  DocumentoSalidaSucursal  String?          @db.VarChar(4)
  DocumentoSalidaNumero    String?          @db.VarChar(8)
  EstadoCodigo             String?          @db.VarChar(2)
  FechaAnulacion           DateTime?        @db.Date
  Descripcion              String?          @db.VarChar(254)
  Observaciones            String?          @db.Text
  cheques_estados          cheques_estados? @relation(fields: [EstadoCodigo], references: [Codigo], onDelete: Restrict, onUpdate: Restrict, map: "cheques_fk")
  t_bancos                 t_bancos?        @relation(fields: [BancoCodigo], references: [Codigo], onDelete: Restrict, onUpdate: Restrict, map: "cheques_fk1")

  @@index([BancoCodigo], map: "BancoCodigo")
  @@index([ClienteCodigo], map: "ClienteCodigo")
  @@index([EstadoCodigo], map: "DestinoCodigo")
  @@index([ProveedorCodigo], map: "ProveedorCodigo")
}

model cheques_estados {
  Codigo      String    @id @unique(map: "Codigo_2") @db.VarChar(2)
  Descripcion String?   @db.VarChar(200)
  cheques     cheques[]

  @@index([Codigo], map: "Codigo")
}

