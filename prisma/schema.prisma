model cheques {
  Codigo                   Int              @id
  Numero                   String?          @db.VarChar(50)
  FechaEntrada             DateTime?        @db.Date
  FechaAcredita            DateTime?        @db.Date
  BancoCodigo              String?          @db.VarChar(20)
  Descripcion              String?          @db.VarChar(254)
  Importe                  Float?           @default(0.00)
  ClienteCodigo            String?          @db.VarChar(10)
  DocumentoEntradaTipo     String?          @db.VarChar(4)
  DocumentoEntradaSucursal String?          @db.VarChar(4)
  DocumentoEntradaNumero   String?          @db.VarChar(8)
  ProveedorCodigo          String?          @db.VarChar(10)
  DocumentoSalidaTipo      String?          @db.VarChar(4)
  DocumentoSalidaSucursal  String?          @db.VarChar(4)
  DocumentoSalidaNumero    String?          @db.VarChar(8)
  EstadoCodigo             String?          @db.VarChar(2)
  FechaAnulacion           DateTime?        @db.Date
  Observaciones            String?          @db.Text
  cheques_estados          cheques_estados? @relation(fields: [EstadoCodigo], references: [Codigo], onDelete: Restrict, onUpdate: Restrict, map: "cheques_fk")
  t_bancos                 t_bancos?        @relation(fields: [BancoCodigo], references: [Codigo], onDelete: Restrict, onUpdate: Restrict, map: "cheques_fk1")

  @@index([BancoCodigo], map: "BancoCodigo")
  @@index([ClienteCodigo], map: "ClienteCodigo")
  @@index([EstadoCodigo], map: "DestinoCodigo")
  @@index([ProveedorCodigo], map: "ProveedorCodigo")
}

model cheques_estados {
  Codigo      String    @id @unique(map: "Codigo_2") @db.VarChar(2)
  Descripcion String?   @db.VarChar(200)
